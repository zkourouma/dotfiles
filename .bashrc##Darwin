export PATH=/usr/local/bin:$PATH

export PATH=/usr/local/share/python:$PATH
export PYTHONDONTWRITEBYTECODE=x
source /usr/local/bin/virtualenvwrapper.sh
export VIRTUALENVWRAPPER_PYTHON=/usr/bin/python
export PYTHONPATH=/usr/lib/python2.7/site-packages/:$HOME/workspace/ssi/:$PYTHONPATH
export PYTHONPATH=/usr/lib/python3.6/site-packages/:$PYTHONPATH

export PATH="$PATH:$HOME/.rvm/bin" # Add RVM to PATH for scripting
export PATH=$PATH:/usr/local/opt/go/libexec/bin
export PATH=$PATH:$HOME/go/third/bin
export PATH="$PATH:$HOME/Library/Haskell/bin"
export PATH="$PATH:$(yarn global dir)/node_modules/.bin"

export JAVA_HOME=$(/usr/libexec/java_home)
export MANPATH="/usr/local/opt/gnu-sed/libexec/gnuman:$MANPATH"
export ANDROID_HOME=/usr/local/opt/android-sdk

export PS1='\[\033[0;36m\]\u\\\[\033[0;32m\]\W\\ \[\033[0;33m\]$(__git_ps1)\[\033[1;37m\]\$\[\033[00m\] '

export NVM_DIR="$HOME/.nvm"
export CLICOLOR=1

[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm
[[ -s "/Users/zack/.gvm/scripts/gvm" ]] && source "/Users/zack/.gvm/scripts/gvm"
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

source "$HOME/scripts/nomad-logs.sh"
export GOROOT_BOOTSTRAP=$GOROOT
export GOPATH="$HOME/go/mine:$HOME/go/third:$GOPATH"
